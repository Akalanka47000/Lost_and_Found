<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="381.0" prefWidth="526.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Wallets">
   <children>
      <Button layoutX="329.0" layoutY="271.0" mnemonicParsing="false" onAction="#saveDetails" style="-fx-background-color: #c9350c;" text="Save Details" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="443.0" layoutY="271.0" mnemonicParsing="false" onAction="#goBacktoFoundOrLost" style="-fx-background-color: #00aaff;" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <VBox layoutX="50.0" layoutY="44.0" prefHeight="227.0" prefWidth="215.0">
         <children>
            <Label text="Wallet colour" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="12.0" />
               </VBox.margin>
            </Label>
            <Label text="ID num if wallet contained ID" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label text="Building found in:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="13.0" />
               </VBox.margin>
            </Label>
            <Label text="Floor found in:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label text="Your first name:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="16.0" />
               </VBox.margin>
            </Label>
            <Label prefHeight="18.0" prefWidth="123.0" text="Contact details:" textFill="WHITE" />
         </children>
      </VBox>
      <VBox layoutX="280.0" layoutY="36.0" prefHeight="194.0" prefWidth="215.0">
         <children>
            <TextField fx:id="txtColor" prefHeight="26.0" prefWidth="183.0" promptText="Ex: black, brown">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtIDNum" prefHeight="26.0" prefWidth="183.0" promptText="Ex: 200215904070">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtBuildingFound" prefHeight="26.0" prefWidth="183.0" promptText="Ex: one, three">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtFloorFound" prefHeight="26.0" prefWidth="183.0" promptText="Ex: ground, first, fifth">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtName" prefHeight="26.0" prefWidth="183.0" promptText="Ex: Kavindu">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtContact" prefHeight="26.0" prefWidth="183.0" promptText="Ex: 0773454556" />
         </children>
      </VBox>
   </children>
</AnchorPane>
