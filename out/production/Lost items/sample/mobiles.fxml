<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="339.0" prefWidth="415.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Mobiles">
   <children>
      <Button layoutX="208.0" layoutY="269.0" mnemonicParsing="false" onAction="#saveDetails" style="-fx-background-color: #c9350c;" text="Save Details" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="320.0" layoutY="269.0" mnemonicParsing="false" onAction="#goBacktoFoundOrLost" style="-fx-background-color: #00aaff;" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <VBox layoutX="50.0" layoutY="35.0" prefHeight="200.0" prefWidth="149.0">
         <children>
            <Label text="Brand:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="13.0" />
               </VBox.margin>
            </Label>
            <Label text="Model:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label text="Building found in:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label text="Floor found in:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="16.0" />
               </VBox.margin>
            </Label>
            <Label text="Your first name:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Label>
            <Label prefHeight="18.0" prefWidth="167.0" text="Contact details:" textFill="WHITE" />
         </children>
      </VBox>
      <VBox layoutX="208.0" layoutY="30.0" prefHeight="186.0" prefWidth="162.0">
         <children>
            <TextField fx:id="txtBrand" promptText="Ex: samsung, apple......">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtModel" promptText="Ex: galaxy_s6, iphone_5">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtBuildingFound" promptText="Ex: one, three">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtFloorFound" promptText="Ex: ground, first , fifth">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtName" promptText="Ex: Kavindu">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtContact" promptText="Ex: 0773454556">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
   </children>
</AnchorPane>
