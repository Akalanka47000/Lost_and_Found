<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="258.0" prefWidth="445.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.WalletSearch">
   <children>
      <Button layoutX="303.0" layoutY="197.0" mnemonicParsing="false" onAction="#Search" style="-fx-background-color: #c9350c;" text="Search" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="379.0" layoutY="197.0" mnemonicParsing="false" onAction="#goBacktoFoundOrLost" style="-fx-background-color: #00aaff;" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <VBox layoutX="247.0" layoutY="34.0" prefHeight="140.0" prefWidth="183.0">
         <children>
            <TextField fx:id="txtColor" prefHeight="26.0" prefWidth="183.0" promptText="Ex: black, brown">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtID" prefHeight="26.0" prefWidth="183.0" promptText="Ex: 200215904070 or -">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtBuildingLostIn" prefHeight="26.0" prefWidth="183.0" promptText="Ex: one, three">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtFloorLastSeen" prefHeight="26.0" prefWidth="183.0" promptText="Ex: ground, first, fifth" />
         </children>
      </VBox>
      <VBox layoutX="38.0" layoutY="44.0" prefHeight="106.0" prefWidth="222.0">
         <children>
            <Label text="Wallet colour" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="14.0" />
               </VBox.margin>
            </Label>
            <Label text="ID num if wallet contained ID" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="14.0" />
               </VBox.margin>
            </Label>
            <Label text="Building lost in:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="14.0" />
               </VBox.margin>
            </Label>
            <Label text="Floor last seen in: " textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
